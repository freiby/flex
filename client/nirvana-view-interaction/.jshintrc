{
    "curly": true, // true: Require {} for every new block or scope 循环或者条件语句必须使用花括号包围
    "eqeqeq": true, // true: Require triple equals (===) for comparison
    "immed": true, // true: Require immediate invocations to be wrapped in parens e.g. `(function () { } ());`
    "latedef": true, // true: Require variables/functions to be defined before being used
    "newcap": true, // true: Require capitalization of all constructor functions e.g. `new F()` 构造器函数首字母大写
    "noarg": true, // true: Prohibit use of `arguments.caller` and `arguments.callee`
    "sub": true, // true: Prohibit use of empty blocks       person['name'] vs. person.name
    "undef": true, // true: Require all non-global variables to be declared (prevents global leaks) 变量未定义
    "boss": false, // true: Require all defined variables be used
    "eqnull": true, // true: Requires all functions run in ES5 Strict Mode
    "node": true, // {int} Max depth of nested blocks (within functions)
    "browser" : true,
    "devel" : true, //定义用于调试的全局变量： console ， alert
    "noempty" : true,
    "asi": true //控制“缺少分号”的警告
}