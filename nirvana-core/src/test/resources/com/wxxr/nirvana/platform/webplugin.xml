<?xml version="1.0" encoding="utf-8" ?>
<plugin
   id="com.wxxr.nirvana.style"
   name="FlexWebUI"
   version="1.0.0"
   provider-name="wxxr.com.cn"
   contextURI="flexWeb"
   messagebundle="styleMessage"
   >
    <extension
         id="nirvanaStyle"
         point="com.wxxr.nirvana.platform.themes">
      <theme
            id="nirvanaStyle_theme"
            name="nirvanaSytle"
            description="nirvanaSytledesc"
			default="true"
            >
			<desktop
				id="nirvanadesktop"
				name="desktop"
				description="desktopdesc"
				uri="desktopuri"
            >
			</desktop>

			<pageLayout
				id="nirvanapagelayout"
				name="layout"
				description="layoutdesc"
				uri="pagelayouturi">
			</pageLayout>
      </theme>
   </extension>


   
</plugin> 
