<?xml version="1.0" encoding="utf-8" ?>
<plugin
   id="com.wxxr.nirvana.style"
   name="FlexWebUI"
   version="1.0.0"
   provider-name="wxxr.com.cn"
   contextURI="flexWeb"
   messagebundle="styleMessage"
   >
    <extension
         id="nirvanaStyle"
         point="com.wxxr.nirvana.platform.themes">
      <theme
            id="nirvanaStyle_theme"
            name="nirvanaSytle"
            description="nirvanaSytledesc"
			default="true"
            >
			<desktop
				id="nirvanadesktop"
				name="desktop"
				description="desktopdesc"
				uri="desktopuri"
            >
			</desktop>

			<resource ref="com.wxxr.nirvana.test.dj" point="header"></resource>
			<resource ref="com.wxxr.nirvana.test.cj" point="header"></resource>
			
      </theme>
	 </extension>

	 <extension
         id="nirvanaStyle"
         point="com.wxxr.nirvana.platform.pagelayouts">
	  <pageLayout
				id="nirvanapagelayout"
				name="layout"
				description="layoutdesc"
				uri="pagelayouturi">
			</pageLayout>


			<pageLayout
				id="nirvanapagelayout2"
				name="layout"
				description="layoutdesc"
				uri="pagelayouturi">
			</pageLayout>
   </extension>


   <extension point="com.wxxr.nirvana.platform.resources" id="pages.niravanaPage">
		  <resource
				id="dj"
				label="tiger"      
				uri="js/d3.min.js"
				type="js"
				>
		  </resource>

		  <resource
				id="cj"
				label="tiger"      
				uri="js/c3.min.js"
				type="js"
				>
		  </resource>


		  
     </extension>


   
</plugin> 
